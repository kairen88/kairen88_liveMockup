package zz.csg.impl;

/**
 * Returns status of the visit method. It permits to control the visit.
 * @author gpothier
 */
public enum GraphicVisitorStatus
{
	/**
	 * The visit should continue
	 */
	CONTINUE, 
	
	/**
	 * The visit should continue, but if the current node is a container,
	 * it shouldn't be visited.
	 */
	DONT_ENTER, 
	
	/**
	 * The visit should stop
	 */
	QUIT;
}