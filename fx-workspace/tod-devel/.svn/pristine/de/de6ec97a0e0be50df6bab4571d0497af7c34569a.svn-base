package net.basekit.shapes.utils;

import javax.vecmath.Tuple3f;


/**
 * Provides access to one of the coordinates of a {@link javax.vecmath.Tuple3f}
 * @author gpothier
 */
public interface CoordinateAccessor
{
	public float get (Tuple3f aTuple);
	public void set (Tuple3f aTuple, float aValue);
}