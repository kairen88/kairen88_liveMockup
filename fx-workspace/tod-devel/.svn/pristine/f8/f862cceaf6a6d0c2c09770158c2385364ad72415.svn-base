package net.basekit.messages;

import net.basekit.Message;
import net.basekit.models.WidgetModel;

/**
 * Message for widget models.
 * @author gpothier
 */
public class WidgetModelMessage extends Message
{
	private WidgetModel itsSource;

	public WidgetModelMessage (WidgetModel aSource)
	{
		itsSource = aSource;
	}

	public WidgetModel getSource ()
	{
		return itsSource;
	}
}
