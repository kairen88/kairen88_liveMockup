package net.basekit.widgets.button;

import net.basekit.widgets.IndividualRenderer;

/**
 * Interface rendering the graphical content of a button.
 * @author gpothier
 */
public abstract class ButtonRenderer extends IndividualRenderer
{
	protected ButtonWidget getButtonWidget ()
	{
		return (ButtonWidget) getRenderedWidget();
	}
}
