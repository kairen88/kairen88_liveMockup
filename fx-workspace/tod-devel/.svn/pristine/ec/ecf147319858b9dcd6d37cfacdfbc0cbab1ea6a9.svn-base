package net.basekit.models.table;

import net.basekit.models.WidgetModel;
import net.basekit.models.list.ListContentWidgetModel;

/**
 * Model for table widgets, where the information is viewed in a tabular form.
 * 
 * @author gpothier
 */
public interface TableWidgetModel extends WidgetModel, TabularWidgetModel
{
	/**
	 * Returns a model representing the content of the table. One element in this model
	 * represents a line in the table.
	 */
	public ListContentWidgetModel getContent ();
}
